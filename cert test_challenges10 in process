    def read_excel(self,table, sheet_name):




        orders = pd.read_excel(table, sheet_name)

    # identification the value of a single cell from column "MAKE"
        make0 = str(orders.iloc[[0]]['MAKE'])
        make1 = str(orders.iloc[[1]]['MAKE'])
        make2 = str(orders.iloc[[2]]['MAKE'])
